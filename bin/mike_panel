#!/bin/bash

gconftool --recursive-unset /apps/panel
gconftool --type list --list-type string --set /apps/panel/general/toplevel_id_list "[panel0]"
gconftool --type list --list-type string --set /apps/panel/general/applet_id_list "[wl0,wss0,sm0,cpu00,cpu01,na0,cl0]"
gconftool --type list --list-type string --set /apps/panel/general/object_id_list "[sep0]"

# panel0
gconftool --type bool --set /apps/panel/toplevels/panel0/enable_arrows true
gconftool --type bool --set /apps/panel/toplevels/panel0/enable_buttons false
gconftool --type bool --set /apps/panel/toplevels/panel0/y_centered false
gconftool --type bool --set /apps/panel/toplevels/panel0/enable_animations true
gconftool --type bool --set /apps/panel/toplevels/panel0/expand true
gconftool --type string --set /apps/panel/toplevels/panel0/name ""
gconftool --type int --set /apps/panel/toplevels/panel0/size 24
gconftool --type int --set /apps/panel/toplevels/panel0/hide_delay 300
gconftool --type bool --set /apps/panel/toplevels/panel0/x_centered false
gconftool --type int --set /apps/panel/toplevels/panel0/x_right 0
gconftool --type int --set /apps/panel/toplevels/panel0/screen 0
gconftool --type int --set /apps/panel/toplevels/panel0/unhide_delay 100
gconftool --type string --set /apps/panel/toplevels/panel0/animation_speed "fast"
gconftool --type int --set /apps/panel/toplevels/panel0/monitor 0
gconftool --type int --set /apps/panel/toplevels/panel0/auto_hide_size 1
gconftool --type int --set /apps/panel/toplevels/panel0/x 0
gconftool --type string --set /apps/panel/toplevels/panel0/orientation "bottom"
gconftool --type int --set /apps/panel/toplevels/panel0/y_bottom 0
gconftool --type bool --set /apps/panel/toplevels/panel0/auto_hide false
gconftool --type int --set /apps/panel/toplevels/panel0/y 0
gconftool --type int --set /apps/panel/toplevels/panel0/background/opacity 6000
gconftool --type bool --set /apps/panel/toplevels/panel0/background/rotate false
gconftool --type string --set /apps/panel/toplevels/panel0/background/type "gtk"
gconftool --type string --set /apps/panel/toplevels/panel0/background/color "#ffffff"
gconftool --type string --set /apps/panel/toplevels/panel0/background/image ""
gconftool --type bool --set /apps/panel/toplevels/panel0/background/fit false
gconftool --type bool --set /apps/panel/toplevels/panel0/background/stretch false
gconftool --apply-schema /schemas/apps/panel/toplevels/enable_arrows /apps/panel/toplevels/panel0/enable_arrows
gconftool --apply-schema /schemas/apps/panel/toplevels/enable_buttons /apps/panel/toplevels/panel0/enable_buttons
gconftool --apply-schema /schemas/apps/panel/toplevels/y_centered /apps/panel/toplevels/panel0/y_centered
gconftool --apply-schema /schemas/apps/panel/toplevels/enable_animations /apps/panel/toplevels/panel0/enable_animations
gconftool --apply-schema /schemas/apps/panel/toplevels/expand /apps/panel/toplevels/panel0/expand
gconftool --apply-schema /schemas/apps/panel/toplevels/name /apps/panel/toplevels/panel0/name
gconftool --apply-schema /schemas/apps/panel/toplevels/size /apps/panel/toplevels/panel0/size
gconftool --apply-schema /schemas/apps/panel/toplevels/hide_delay /apps/panel/toplevels/panel0/hide_delay
gconftool --apply-schema /schemas/apps/panel/toplevels/x_centered /apps/panel/toplevels/panel0/x_centered
gconftool --apply-schema /schemas/apps/panel/toplevels/x_right /apps/panel/toplevels/panel0/x_right
gconftool --apply-schema /schemas/apps/panel/toplevels/screen /apps/panel/toplevels/panel0/screen
gconftool --apply-schema /schemas/apps/panel/toplevels/unhide_delay /apps/panel/toplevels/panel0/unhide_delay
gconftool --apply-schema /schemas/apps/panel/toplevels/animation_speed /apps/panel/toplevels/panel0/animation_speed
gconftool --apply-schema /schemas/apps/panel/toplevels/monitor /apps/panel/toplevels/panel0/monitor
gconftool --apply-schema /schemas/apps/panel/toplevels/auto_hide_size /apps/panel/toplevels/panel0/auto_hide_size
gconftool --apply-schema /schemas/apps/panel/toplevels/x /apps/panel/toplevels/panel0/x
gconftool --apply-schema /schemas/apps/panel/toplevels/orientation /apps/panel/toplevels/panel0/orientation
gconftool --apply-schema /schemas/apps/panel/toplevels/y_bottom /apps/panel/toplevels/panel0/y_bottom
gconftool --apply-schema /schemas/apps/panel/toplevels/auto_hide /apps/panel/toplevels/panel0/auto_hide
gconftool --apply-schema /schemas/apps/panel/toplevels/y /apps/panel/toplevels/panel0/y
gconftool --apply-schema /schemas/apps/panel/toplevels/background/opacity /apps/panel/toplevels/panel0/background/opacity
gconftool --apply-schema /schemas/apps/panel/toplevels/background/rotate /apps/panel/toplevels/panel0/background/rotate
gconftool --apply-schema /schemas/apps/panel/toplevels/background/type /apps/panel/toplevels/panel0/background/type
gconftool --apply-schema /schemas/apps/panel/toplevels/background/color /apps/panel/toplevels/panel0/background/color
gconftool --apply-schema /schemas/apps/panel/toplevels/background/image /apps/panel/toplevels/panel0/background/image
gconftool --apply-schema /schemas/apps/panel/toplevels/background/fit /apps/panel/toplevels/panel0/background/fit
gconftool --apply-schema /schemas/apps/panel/toplevels/background/stretch /apps/panel/toplevels/panel0/background/stretch
# seperator (sep0)
gconftool --type string --set /apps/panel/objects/sep0/menu_path "applications:/"
gconftool --type string --set /apps/panel/objects/sep0/launcher_location ""
gconftool --type string --set /apps/panel/objects/sep0/bonobo_iid ""
gconftool --type string --set /apps/panel/objects/sep0/custom_icon ""
gconftool --type bool --set /apps/panel/objects/sep0/locked true
gconftool --type bool --set /apps/panel/objects/sep0/panel_right_stick true
gconftool --type string --set /apps/panel/objects/sep0/object_type "separator"
