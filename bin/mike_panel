gconftool --apply-schema /schemas/apps/panel/objects/launcher_location /apps/panel/objects/sep0/launcher_location
gconftool --apply-schema /schemas/apps/panel/objects/bonobo_iid /apps/panel/objects/sep0/bonobo_iid
gconftool --apply-schema /schemas/apps/panel/objects/custom_icon /apps/panel/objects/sep0/custom_icon
gconftool --apply-schema /schemas/apps/panel/objects/locked /apps/panel/objects/sep0/locked
gconftool --apply-schema /schemas/apps/panel/objects/panel_right_stick /apps/panel/objects/sep0/panel_right_stick
gconftool --apply-schema /schemas/apps/panel/objects/object_type /apps/panel/objects/sep0/object_type
gconftool --apply-schema /schemas/apps/panel/objects/use_custom_icon /apps/panel/objects/sep0/use_custom_icon
gconftool --apply-schema /schemas/apps/panel/objects/tooltip /apps/panel/objects/sep0/tooltip
gconftool --apply-schema /schemas/apps/panel/objects/toplevel_id /apps/panel/objects/sep0/toplevel_id
gconftool --apply-schema /schemas/apps/panel/objects/action_type /apps/panel/objects/sep0/action_type
gconftool --apply-schema /schemas/apps/panel/objects/use_menu_path /apps/panel/objects/sep0/use_menu_path
gconftool --apply-schema /schemas/apps/panel/objects/position /apps/panel/objects/sep0/position
gconftool --apply-schema /schemas/apps/panel/objects/attached_toplevel_id /apps/panel/objects/sep0/attached_toplevel_id
# window list (wl0)
gconftool --type string --set /apps/panel/applets/wl0/menu_path "applications:/"
gconftool --type string --set /apps/panel/applets/wl0/launcher_location "" 
gconftool --type string --set /apps/panel/applets/wl0/bonobo_iid "OAFIID:GNOME_WindowListApplet"
