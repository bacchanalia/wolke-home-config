#!/usr/bin/perl
use strict;
use warnings;

my $file = shift;

open INFH, $file or
die "Error opening $file for reading\n";
my @lines = <INFH>;
close INFH;

foreach my $x (@lines){
  my $count=0;
  foreach my $y (@lines){
    if($x eq $y){
      $count++;
    }
  }
  chomp $x;
  if($count != 1){
    print "'$x' occurred $count times\n";
  }
}

