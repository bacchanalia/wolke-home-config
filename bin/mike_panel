gconftool --apply-schema /schemas/apps/panel/objects/use_custom_icon /apps/panel/applets/wl0/use_custom_icon
gconftool --apply-schema /schemas/apps/panel/objects/tooltip /apps/panel/applets/wl0/tooltip
gconftool --apply-schema /schemas/apps/panel/objects/toplevel_id /apps/panel/applets/wl0/toplevel_id
gconftool --apply-schema /schemas/apps/panel/objects/action_type /apps/panel/applets/wl0/action_type
gconftool --apply-schema /schemas/apps/panel/objects/use_menu_path /apps/panel/applets/wl0/use_menu_path
gconftool --apply-schema /schemas/apps/panel/objects/position /apps/panel/applets/wl0/position
gconftool --apply-schema /schemas/apps/panel/objects/attached_toplevel_id /apps/panel/applets/wl0/attached_toplevel_id
gconftool --apply-schema /schemas/apps/window_list_applet/prefs/display_all_workspaces /apps/panel/applets/wl0/prefs/display_all_workspaces
gconftool --apply-schema /schemas/apps/window_list_applet/prefs/group_windows /apps/panel/applets/wl0/prefs/group_windows
gconftool --apply-schema /schemas/apps/window_list_applet/prefs/maximum_size /apps/panel/applets/wl0/prefs/maximum_size
