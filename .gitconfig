[alias]
	s = status
	stat = diff --stat
	unstage = reset HEAD
	vimdiff = difftool -t vimdiff
	meld = difftool -t meld
	graph = log --graph --oneline --color
	ignored = ls-files --others -i --exclude-standard
[color]
	status = true
[push]
	default = simple
