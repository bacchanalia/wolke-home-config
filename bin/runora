#!/bin/bash
echo YOU MEANT TO LOG IN TO DEV, RIGHT? doin that for ya
ssh ewolk@dev.escribe.com \
  sudo -u oracle \
    ORACLE_OWNER=oracle \
    ORACLE_HOME=/oracle/program/oracle/10.2.0/db_1 \
    ORACLE_USER=oracle \
    PATH=$PATH:$ORACLE_HOME/bin:/opt/escribe/bin \
    NLS_LANG=AMERICAN_AMERICA.WE8ISO8859P1 \
    LD_LIBRARY_PATH={$LD_LIBRARY_PATH}:$ORACLE_HOME/lib \
    /opt/escribe/bin/gqlplus ESCRIBE/ESCRIBE1523
